#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/blogs/myblog/Blog/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/mnt/d/blogs/myblog/Blog/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/mnt/d/blogs/myblog/Blog/node_modules/.pnpm/esprima@4.0.1/node_modules:/mnt/d/blogs/myblog/Blog/node_modules/.pnpm/node_modules:/mnt/d/blogs/myblog/Blog/node_modules:/mnt/d/blogs/myblog/node_modules:/mnt/d/blogs/node_modules:/mnt/d/node_modules:/mnt/d/blogs/myblog/Blog/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/mnt/d/blogs/myblog/Blog/node_modules/.pnpm/js-yaml@3.14.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/blogs/myblog/Blog/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/mnt/d/blogs/myblog/Blog/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/mnt/d/blogs/myblog/Blog/node_modules/.pnpm/esprima@4.0.1/node_modules:/mnt/d/blogs/myblog/Blog/node_modules/.pnpm/node_modules:/mnt/d/blogs/myblog/Blog/node_modules:/mnt/d/blogs/myblog/node_modules:/mnt/d/blogs/node_modules:/mnt/d/node_modules:/mnt/d/blogs/myblog/Blog/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/mnt/d/blogs/myblog/Blog/node_modules/.pnpm/js-yaml@3.14.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esvalidate.js" "$@"
fi
